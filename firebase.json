{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/c/**",
        "destination": "/c/[id].html"
      },
      {
        "source": "/validate/**",
        "destination": "/validate.html"
      }
    ]
  }
}
