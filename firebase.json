{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/ahmed",
        "destination": "/admin.html"
      },
      {
        "source": "/preview",
        "destination": "/admin.html"
      },
      {
        "source": "/c/:id",
        "destination": "/certificate.html?id=:id"
      },
      {
        "source": "/validate/**",
        "destination": "/validate.html"
      }
    ]
  }
}
